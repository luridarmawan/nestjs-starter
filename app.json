{
  "name": "nestjs-starter",
  "repository": "https://github.com/thisismydesign/nestjs-starter",
  "env": {
    "NODE_ENV": {
      "value": "production"
    },
    "JWT_SECRET": {
      "generator": "secret"
    },
    "JWT_EXPIRES_IN": {
      "value": "10d"
    },
    "OAUTH_GOOGLE_ID": {
      "value": "dummy"
    },
    "OAUTH_GOOGLE_SECRET": {
      "value": "dummy"
    },
    "OAUTH_GOOGLE_REDIRECT_URL": {
      "value": "dummy"
    },
    "OAUTH_COGNITO_ID": {
      "value": "dummy"
    },
    "OAUTH_COGNITO_SECRET": {
      "value": "dummy"
    },
    "OAUTH_COGNITO_REDIRECT_URL": {
      "value": "dummy"
    },
    "OAUTH_COGNITO_DOMAIN": {
      "value": "dummy"
    },
    "OAUTH_COGNITO_REGION": {
      "value": "us-east-1"
    },
    "NEXT_PUBLIC_GA_MEASUREMENT_ID": {
      "value": "dummy"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "13"
      }
    }
  ],
  "stack": "container"
}
